%
% Test breakpoints.
%
clear
set position 0
load "+++++>>"
break at 2
%-Breakpoint 1 added at 2.
run
%-Program stopped at breakpoint.
show
%-PC is 2, tape position is 0.
%-Breakpoint 1 set at 2.
%-Left end of tape: infinite.
%-Right end of tape: infinite.
%-Cell lower bound: infinite.
%-Cell upper bound: infinite.
%-Input method: unsigned char.
%-Output method: char.
%-EOF processing: value -1.
run
%-Program terminated normally.
show
%-PC is 7, tape position is 2.
%-Breakpoint 1 set at 2.
%-Left end of tape: infinite.
%-Right end of tape: infinite.
%-Cell lower bound: infinite.
%-Cell upper bound: infinite.
%-Input method: unsigned char.
%-Output method: char.
%-EOF processing: value -1.
break at 1
%-Breakpoint 2 added at 1.
break at 3
%-Breakpoint 3 added at 3.
set pc 0
set position 0
clear
show
%-PC is 0, tape position is 0.
%-Breakpoint 1 set at 2.
%-Breakpoint 2 set at 1.
%-Breakpoint 3 set at 3.
%-Left end of tape: infinite.
%-Right end of tape: infinite.
%-Cell lower bound: infinite.
%-Cell upper bound: infinite.
%-Input method: unsigned char.
%-Output method: char.
%-EOF processing: value -1.
run
%-Program stopped at breakpoint.
show tape 1
%->>> 0 1
run
%-Program stopped at breakpoint.
show tape 1
%->>> 0 2
run
%-Program stopped at breakpoint.
show tape 1
%->>> 0 3
run
%-Program terminated normally.
show tape 5
%-    0 5
%-    1 0
%->>> 2 0
%-    3 0
%-    4 0
delete 5
%-Breakpoints are numbered from 1 to 3.
delete -1
%-Breakpoints are numbered from 1 to 3.
%
% Test `set tape', `run...'
%
set position 0
clear
set tape 0 65
set tape 1 66
set tape 2 67
set tape 3 68
set tape 4 69
load "[.>]"
run for 4
%
% The first time through the loop, we execute both [ and ].
%
%-AFinished executing 4 instructions.
%
% Successive executions skip the [.
%
run for 6
%-BCFinished executing 6 instructions.
show
%-PC is 1, tape position is 3.
%-Left end of tape: infinite.
%-Right end of tape: infinite.
%-Cell lower bound: infinite.
%-Cell upper bound: infinite.
%-Input method: unsigned char.
%-Output method: char.
%-EOF processing: value -1.
run
%-DEProgram terminated normally.
show
%-PC is 4, tape position is 5.
%-Left end of tape: infinite.
%-Right end of tape: infinite.
%-Cell lower bound: infinite.
%-Cell upper bound: infinite.
%-Input method: unsigned char.
%-Output method: char.
%-EOF processing: value -1.
%
% `run at', `read', decimal input & output, bignums
%
read "test.b"
clear
set position 0
set input decimal
set output decimal
run at 10
